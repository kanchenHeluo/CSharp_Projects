<div>
    <div class="clr layout-gapS"></div>
    <div>
        <div class="clr layout-scroll maxheightS">
            <table class="table table-agreement table-hover table-bordered" ng-if="agreementSearchResults !== null && agreementSearchResults.length > 0">
                <thead class="nowrap">
                    <tr class="ms-stdlabel">
                        <th>Agreement Number</th>
                        <th>Public Customer Number</th>
                        <th>Customer Name</th>
                        <th>Country/Region</th>
                        <th width="180">Agreement Duration</th>
                    </tr>
                </thead>
                <tbody class="ms-stdtext">
                    <tr ng-repeat="item in agreementSearchResults" ng-class="{selected: item.selected}" ng-click="agreementValidateAction($event, item, 'td')">
                        <td>{{item.AgreementNumber}}</td>
                        <td>{{item.EndCustomerNumber}}</td>
                        <td>{{item.EndCustomerName}}</td>
                        <td>{{item.CountryName}}</td>
                        <td>{{item.Duration}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <op-dialog title="Selecting a new agreement will re-create a new order, continue?" ng-open="dialogStatus.selectNewAgreementDialogOpen">
        <div class="text-center">
            <div class="layout-gap" />
            <div style="text-align:center">
                <input type="button" class="ms-stdbtn" ng-click="selectNewAgreement()" value="Select" />
                <input type="button" class="ms-stdbtn" ng-click="cancelSelection()" value="Cancel" style="margin-left:100px" />
            </div>
            <div class="layout-gap" />
        </div>
    </op-dialog>
</div>